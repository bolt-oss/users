{
  "name": "bolt-oss/users",
  "description": "Bolt 4 Users Extension",
  "type": "bolt-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivo Valchev",
      "email": "me@ivovalchev.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "twig/twig": "^2.12 | ^3.0"
  },
  "require-dev": {
    "bolt/core": "^4.0.0",
    "symplify/easy-coding-standard": "^8.1",
    "phpstan/phpstan": "^0.12.32",
    "phpstan/phpstan-symfony": "^0.12.6"
  },
  "replace": {
    "bolt/users": "*"
  },
  "autoload": {
    "psr-4": {
      "Bolt\\UsersExtension\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "entrypoint": "Bolt\\UsersExtension\\Extension"
  }
}
